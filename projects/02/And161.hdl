/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP And161 {
    IN a[16], b;
    OUT out;

    PARTS:
    // Put your code here:
    // MSG is signed bit. return MSB
    And(a=a[15], b=b, out=out);

}